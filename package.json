{
  "name": "react-redux-typescript-starter-kit",
  "version": "0.12.0",
  "description": "Futuristic, production-ready development environment for building Component-Driven, Flux Single Page Applications with React, Redux and TypeScript - powered by JSPM (SystemJS with Hot-Reload & Rollup).",
  "author": "Piotr Witek <piotrek.witek@gmail.com> (http://piotrwitek.github.io/)",
  "homepage": "http://piotrwitek.github.io/react-redux-typescript-starter-kit/",
  "repository": "https://github.com/piotrwitek/react-redux-typescript-starter-kit.git",
  "bugs": "https://github.com/piotrwitek/react-redux-typescript-starter-kit/issues",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "dev": "webpack-dev-server --progress --hot",
    "build": "npm run build:dll && npm run build:app",
    "build:dll": "webpack --progress --config webpack.dll.config.js",
    "build:app": "webpack --progress --config webpack.config.js",
    "lint": "tslint --project tsconfig.json --type-check",
    "lint:fix": "tslint --fix --project tsconfig.json --type-check",
    "test": "jest --config jest.json",
    "test:update": "jest --config jest.json -u"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/classnames": "^0.0.32",
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.8",
    "@types/react": "^15.0.16",
    "@types/react-bootstrap": "^0.0.47",
    "@types/react-dom": "^0.14.20",
    "@types/react-redux": "^4.4.38",
    "@types/react-router": "^3.0.5",
    "@types/react-router-redux": "^4.0.41",
    "awesome-typescript-loader": "^3.1.2",
    "classnames": "^2.2.5",
    "csjs": "^1.1.0",
    "css-loader": "^0.28.0",
    "insert-css": "^2.0.0",
    "jest": "^19.0.2",
    "react": "^15.5.4",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.5.4",
    "react-hot-loader": "3",
    "react-redux": "4",
    "react-redux-typescript": "^2.2.0",
    "react-router": "3",
    "react-router-redux": "4",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "reselect": "^2.5.4",
    "rxjs": "^5.2.0",
    "ts-jest": "^19.0.1",
    "tslib": "^1.6.0",
    "tslint": "^5.1.0",
    "tslint-config-standard": "^5.0.2",
    "typescript": "^2.2.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
